<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Description</th>
      <th>Jedi</th>
      <th>Price</th>
      <th>Stock quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.model %></td>
        <td><%= product.description %></td>
        <td><%= product.jedi %></td>
        <td><%= product.price %></td>
        <td><%= product.stock_quantity %></td>
        <td><%= link_to 'Show', product %></td>
      </tr>
    <% end %>
  </tbody>
</table>